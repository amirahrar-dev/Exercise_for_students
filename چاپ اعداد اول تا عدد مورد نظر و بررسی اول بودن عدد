# چاپ اعداد اول از صفر تا عدد مورد نظر
for i in range(2,int(input("تا کدام عدد"))):
    m=0
    for j in range(2,int(i**(0.5))+1):
        if i%j==0:m+=1;break
    if m==0:print(i)


#===========================================================
# بررسی اول ویا مرکب بودن عدد مورد نظر

a=int(input("تا کدام عدد"))
m=0
for i in range(2,int(a**(0.5))+1):
    if a%i==0:m+=1;break

print(i,"عددی اول",["است","نیست"][m])
